#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0; i<t; i++){
	    int w,x,y,z;
	    cin>>w>>x>>y>>z;
	    if(w==x || w==y || w==z){
	        cout<<"YES"<<endl;
	    }
	    else if(x+y==w || y+z==w || z+x==w){
	        cout<<"YES"<<endl;
	    }
	    else if(x+y+z==w){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
