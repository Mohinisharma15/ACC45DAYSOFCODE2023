#include <iostream>
#include <math.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0; i<t; i++){
	    int n;
	    cin>>n;
	    int count=0,lar=0;
	    while(n>0){
	        lar=sqrt(n);
	        n=n-lar*lar;
	        count++;
	    }
	    cout<<count<<endl;
	}
	return 0;
}
