#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0; i<t; i++){
	     int num;
	     cin>>num;
	     if(num==1){
	         cout<<"YES"<<endl;
	     }
	     else if(num%2==0){
	         cout<<"YES"<<endl;
	     }
	     else{
	         cout<<"NO"<<endl;
	     }
	}
	return 0;
}
