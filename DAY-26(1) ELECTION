#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int num,t,vote;
	cin>>t;
	for(int i=0; i<t; i++){
	    cin>>num>>vote;
	    int a[num],count=0;
	    for(int i=0; i<num; i++){
	        cin>>a[i];
	        if(a[i]>=vote){
	            count++;
	        }
	        
	    }
	    cout<<count<<endl;
	}
	return 0;
}
