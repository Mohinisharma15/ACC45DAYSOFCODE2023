#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int j=0; j<t; j++){
	    int a,b;
	    cin>>a>>b;
	    int temp=0,temp1=0;
	    int i=1;
	    while(temp<=a && temp1<=b){
	        if(i%2!=0){
	            temp=temp+i;
	        }
	        else{
	            temp1=temp1+i;
	        }
	        i++;
	    }
	    if(temp<=a){
	        cout<<"Limak"<<endl;
	    }
	    else{
	        cout<<"Bob"<<endl;
	    }
	}
	
	return 0;
}
