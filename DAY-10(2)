
#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    for(int i=0; i<t; i++){
    int a,b,c,d,e;
    cin>>a>>b>>c>>d>>e;
    if(a+b<=d && c<=e){
        cout<<"YES"<<endl;
    }
    else if(a+c<=d && b<=e){
        cout<<"YES"<<endl;
    }
    else if(c+b<=d && a<=e){
        cout<<"YES"<<endl;
    }
    else{
        cout<<"NO"<<endl;
    }
	
}
	    
	return 0;
}
