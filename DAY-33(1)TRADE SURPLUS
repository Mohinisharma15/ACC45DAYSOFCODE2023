#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0; i<t; i++){
	int a1,a2,b1,b2;
	cin>>a1>>a2>>b1>>b2;
	int temp1=a1-a2;
	int temp2=b1-b2;
	if(temp1+temp2>0){
	    cout<<"NO"<<endl;
	}
	else if(temp1+temp2<0){
	    cout<<"YES"<<endl;
	}
	else{
	    cout<<"NO"<<endl;
	}
	
	}
	return 0;
}
