#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0; i<t; i++){
	    int a,b,x,y;
	    cin>>a>>b>>x>>y;
	    if(b>a){
	        int temp=b-a;
	        if(temp<=x){
	            cout<<"YES"<<endl;
	        }
	        else{
	            cout<<"NO"<<endl;
	        }
	    }
	    else if(a>b){
	        int temp=a-b;
	        if(temp<=y){
	            cout<<"YES"<<endl;
	        }
	        else{
	            cout<<"NO"<<endl;
	        }
	    }
	    else{
	        cout<<"YES"<<endl;
	    }
	}
	return 0;
}
