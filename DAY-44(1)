#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0; i<t; i++){
	    int num,count=0;
	    cin>>num;
	    for(int i=2; i<=num; i++){
	        if(num%i==0){
	            count++;
	            
	        }
	    }
	    if(count==1){
	        cout<<"yes"<<endl;
	    }
	    else{
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
