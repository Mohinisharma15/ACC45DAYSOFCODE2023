#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0; i<t; i++){
	    int x,y,z;
	    cin>>x>>y>>z;
	    int temp=0;
	    if(x>y){
	        temp=x-y;
	    }
	    else{
	        temp=y-x;
	    }
	    if(z>=temp){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
