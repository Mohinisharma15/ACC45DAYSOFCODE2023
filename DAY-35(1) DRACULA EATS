#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0; i<t; i++){
	    int num;
	    cin>>num;
	    if(num<1){
	        cout<<0<<endl;
	    }
	    else if(num>1 && num<7){
	        cout<<1<<endl;
	    }
	    else if(num%7<=1){
	      cout<<num/7<<endl;
	      } 
	      else{
	          cout<<(num/7)+1<<endl;
	      }
	}
	return 0;
}
