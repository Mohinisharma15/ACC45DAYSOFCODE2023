#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0; i<t; i++){
	    int s,x,y,z;
	    cin>>s>>x>>y>>z;
	    
	    if(x+y+z<=s){
	        cout<<"0"<<endl;
	    }
	    else if(s-x>=z || s-y>=z){
	        cout<<"1"<<endl;
	    }
	    else{
	        cout<<"2"<<endl;
	    }
	}
	return 0;
}
